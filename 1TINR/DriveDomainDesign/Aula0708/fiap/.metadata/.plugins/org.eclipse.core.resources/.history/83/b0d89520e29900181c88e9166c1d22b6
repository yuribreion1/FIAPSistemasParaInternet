	 * 
	 */
	private static final long serialVersionUID = 8049692509336543579L;

	public String trataExcecao(Exception e) {
		if (e.getClass().getName().equals("java.lang.NumberFormatException")) {
			
		}
		return "Deu ruim";
	}
}
